<p id="notice"><%= notice %></p>

<h1>Caças Imperiais Disponíveis</h1>

<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Modelo</th>
      <th>Uuid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aeronaves.each do |aeronave| %>
      <tr>
        <td><%= aeronave.modelo %></td>
        <td><%= aeronave.uuid %></td>
        <td><%= link_to 'Detalhes', aeronave %></td>
        <td><%= link_to 'Editar', edit_aeronave_path(aeronave) %></td>
        <td><%= link_to 'Destroy', aeronave, method: :delete, data: { confirm: 'Tem certeza ?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Caça Imperial', new_aeronave_path, :class => "btn btn-primary" %>
